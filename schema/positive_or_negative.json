{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The Root Schema",
    "description": "The root schema comprises the entire JSON document.",
    "required": [
        "data",
        "last_update"
    ],
    "properties": {
        "data": {
            "$id": "#/properties/data",
            "type": "array",
            "title": "The Data Schema",
            "description": "An explanation about the purpose of this instance.",
            "default": [],
            "items": {
                "$id": "#/properties/data/items",
                "type": "object",
                "title": "The Items Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": {},
                "examples": [
                    {
                        "日付": "2020-01-01T00:00:00+09:00",
                        "陽性数": 0,
                        "陰性数": 0,
                        "7日間平均陽性率": 0.0
                    },
                    {
                        "日付": "2020-01-02T00:00:00+09:00",
                        "陽性数": 0,
                        "陰性数": 0,
                        "7日間平均陽性率": 0.0
                    }
                ],
                "required": [
                    "日付"
                ],
                "properties": {
                    "日付": {
                        "$id": "#/properties/data/items/properties/日付",
                        "type": "string",
                        "title": "The 日付 Schema",
                        "description": "An explanation about the purpose of this instance.",
                        "default": "",
                        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}[+][0-9]{2}:[0-9]{2}$",
                        "examples": [
                            "2020-01-01T00:00:00+09:00"
                        ]
                    },
                    "陽性数": {
                        "$id": "#/properties/data/items/properties/陽性数",
                        "type": "integer",
                        "title": "The 陽性数 Schema",
                        "description": "An explanation about the purpose of this instance.",
                        "default": 0,
                        "examples": [
                            0
                        ]
                    },
                    "陰性数": {
                        "$id": "#/properties/data/items/properties/陰性数",
                        "type": "integer",
                        "title": "The 陰性数 Schema",
                        "description": "An explanation about the purpose of this instance.",
                        "default": 0,
                        "examples": [
                            0
                        ]
                    },
                    "7日間平均陽性数": {
                        "$id": "#/properties/data/items/properties/7日間平均陽性数",
                        "type": [
                            "number",
                            "null"
                        ],
                        "title": "The 7日間平均陽性数 Schema",
                        "description": "An explanation about the purpose of this instance.",
                        "default": 0.0,
                        "examples": [
                            0.0
                        ]
                    },
                    "7日間平均陽性率": {
                        "$id": "#/properties/data/items/properties/7日間平均陽性率",
                        "type": [
                            "number",
                            "null"
                        ],
                        "title": "The 7日間平均陽性率 Schema",
                        "description": "An explanation about the purpose of this instance.",
                        "default": 0.0,
                        "examples": [
                            0.0
                        ]
                    }
                }
            }
        },
        "last_update": {
            "$id": "#/properties/last_update",
            "type": "string",
            "title": "The Last_update Schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}[+][0-9]{2}:[0-9]{2}$",
            "examples": [
                "2020-01-01T00:00:00+09:00"
            ]
        }
    }
}